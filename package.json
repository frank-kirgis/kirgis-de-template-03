{
    "title": "Kirgis template",
    "name": "Kirgis",
    "version": "1.0.0",
    "scripts": {
        "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets && npm run build:media",
        "build:assets": "node scripts/build-assets.js",
        "build:media": "node scripts/build-media.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "A HTML website template built with Bootstrap",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://www.kirgis.de",
    "bugs": {
        "url": "https://github.com/fk-caupo/kirgis-de-template-03/issues",
        "email": "frank.kirgis@gmail.com"
    },
    "license": "MIT",
