    "contributors": [
        "Frank Kirgis (https://www.e-commerce-manager.eu/)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/fk-caupo/kirgis-de-template-03.git"
    },
    "dependencies": {
        "bootstrap": "5.2.3"
    },
    "devDependencies": {
        "autoprefixer": "10.4.13",
        "browser-sync": "2.27.11",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "postcss": "8.4.20",
        "prettier": "2.8.1",
        "pug": "3.0.2",
        "sass": "1.57.1",
        "shelljs": "0.8.5",
        "upath": "2.0.1"
    }
}
